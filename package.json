    "webpack-dev-server": "^3.1.8",
    "xml": "^1.0.1"
  },
  "dependencies": {
    "@magento/peregrine": "file:packages/peregrine",
    "@magento/pwa-buildpack": "file:packages/pwa-buildpack",
    "@magento/upward-js": "file:packages/upward-js",
    "@magento/upward-spec": "file:packages/upward-spec",
    "pwa-devdocs": "file:packages/pwa-devdocs",
    "theme-frontend-venia": "file:packages/venia-concept"
  }
}
