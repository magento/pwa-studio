    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "storybook-readme": "^3.3.0",
    "strip-ansi": "^4.0.0",
    "style-loader": "^0.23.0",
    "supertest": "^3.1.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^4.18.0",
    "webpack-assets-manifest": "^3.0.2",
