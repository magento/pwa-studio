{
  "name": "@magento/venia-product-recommendations",
  "version": "1.0.1",
  "main": "./lib/index.js",
  "license": "MIT",
  "pwa-studio": {
    "targets": {
      "intercept": "./lib/targets/intercept.js"
    }
  },
  "files": [
    "/lib"
  ],
  "scripts": {
    "cm": "cz",
    "clean": " ",
    "format": "npm run prettier -- --write",
    "lint": "eslint 'lib/**/*'",
    "lint:fix": "npm run lint -- --fix",
    "lint-staged": "lint-staged",
    "prettier": "prettier \"**/*.+(ts|tsx|js|json)\"",
    "tessa": "tessa-npm-plugin check --errorOnNewVulnerability",
    "tessa:update": "tessa-npm-plugin update",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "release": "cross-env HUSKY_SKIP_HOOKS=1 standard-version"
  },
  "peerDependencies": {
    "@magento/peregrine": "~10.0.0",
    "@magento/pwa-buildpack": "~9.0.0",
    "@magento/venia-ui": "~7.0.0",
    "eslint-plugin-react": "~7.21.5",
    "eslint-plugin-react-hooks": "~2.0.1",
    "react": "~16.9.0",
    "react-router-dom": "~5.1.0"
  },
  "devDependencies": {
    "@adobe/adobe-client-data-layer": "1.1.5",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "7.12.13",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@digitalroute/cz-conventional-changelog-for-jira": "6.0.2",
    "@magento/babel-preset-peregrine": "1.1.0",
    "@magento/peregrine": "~10.0.0",
    "@magento/venia-concept": "~14.1.0",
    "@magento/venia-ui": "7.0.0",
    "@testing-library/react-hooks": "5.0.3",
    "@types/jest": "26.0.20",
    "apollo-cache-persist": "0.1.1",
    "babel-jest": "26.6.3",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-graphql-tag": "3.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "commitizen": "4.2.2",
    "cross-env": "7.0.3",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-react-hooks": "4.1.2",
    "husky": "^4.3.8",
    "jest": "26.6.3",
    "jest-transform-graphql": "2.1.0",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "react-refresh": "^0.9.0",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "17.0.1",
    "redux": "4.0.5",
    "standard-version": "9.1.1"
  },
  "lint-staged": {
    "**/*.+(ts|tsx|js)": [
      "npm run lint:fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira"
    }
  },
  "dependencies": {
    "@magento/recommendations-js-sdk": "^1.1.3",
    "@magento/venia-data-collector": "^1.0.7",
    "libvips": "~0.0.2",
    "prop-types": "^15.7.2",
    "sharp": "~0.33.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/magento-commerce/data-services-pwa-recs.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
