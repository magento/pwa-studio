{
	"name": "PWA Studio",
	"image": "mcr.microsoft.com/vscode/devcontainers/javascript-node:0-14",
	"settings": {},
	"extensions": [
		"EditorConfig.EditorConfig",
		"jeremyrajan.webpack",
		"dbaeumer.vscode-eslint",
		"esbenp.prettier-vscode",
		"graphql.vscode-graphql",
		"bradlc.vscode-tailwindcss"
	],
	"forwardPorts": [8080],
	"postStartCommand": "yarn run watch",
	"postCreateCommand": "cp ./.devcontainer/.env.sample ./.env && yarn install && yarn run build:dev",
	"remoteUser": "node",
	"features": {}
}
